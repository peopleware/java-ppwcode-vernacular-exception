<tag>
  <display-name>ppw_form:crudButtons</display-name>
  <name>crudButtons</name>
  <description>
    Shows the CRUD buttons for a form.
  </description>
  <example>
    &lt;ppw_form:crudButtons &quot;${actionForm}&quot;/&gt;
  </example>
  <body-content>empty</body-content>
  <attribute>
    <name>actionForm</name>
    <description>
      The struts action form containing all object properties that can be used.
    </description>
    <type>be.peopleware.web.struts.PersistentBeanDynaActionForm</type>
    <required>true</required>
    <rtexprvalue>true</rtexprvalue>
  </attribute>

